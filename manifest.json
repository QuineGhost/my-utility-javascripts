{
  "name": "Fast Google Search",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "My Utility JavaScripts.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "*://*/*"],
      "js": ["main.js"]
    }
  ]
}
